{
  "name": "unitdev-website",
  "version": "2.0.0",
  "description": "Professional programming company website with React SSR, dark mode theme",
  "main": "dist/server.js",
  "scripts": {
    "build:client": "webpack --config webpack.client.js --mode production",
    "build:server": "webpack --config webpack.server.js --mode production",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "webpack --config webpack.client.js --mode development --watch",
    "dev:server": "nodemon --watch src --exec babel-node src/server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "node dist/server.js"
  },
  "keywords": [
    "react",
    "ssr",
    "webpack",
    "express",
    "dark-mode"
  ],
  "author": "UnitDev",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-helmet-async": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
